

<!-- creation du jardin 40.times => 4metres, etc. -->
<div class="garden-container"  id="example3Right">
  <% 30.times do |index_y| %>
    <% 40.times do |index_x| %>
      <div class="<%=index_x%>x <%=index_y%>y garden-units"><p><%= index_y %>-<%= index_x %></p></div>

    <% end %>
  <% end %>
</div>

