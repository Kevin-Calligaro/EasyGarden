

<!-- creation du jardin 40.times => 4metres, etc. -->
<div class="garden-container">
  <% 40.times do |index_y| %>
    <% 30.times do |index_x| %>
      <div class="<%=index_x%>x <%=index_y%>y garden-units "></div>
    <% end %>
  <% end %>
</div>

