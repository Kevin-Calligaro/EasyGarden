
<section id="Collidable" class="Collidable">
  <article class="BlockLayout BlockLayout--typePositioned" tabindex="0">



<!-- creation du jardin 40.times => 4metres, etc. -->
                  <div class="garden-container">
                    <% 20.times do |index_y| %>
                      <% 15.times do |index_x| %>
                        <div class="BlockWrapper BlockWrapper--isDropzone garden-units">
                          <span class="garden-square"><div class="BlockContent"></div></span>
                        </div>
                      <% end %>
                    <% end %>
                </div>

<h1>Ma reserve de l√©gume</h1>
<!-- LEGUME A PLANTER -->
                  <div class="vegetable-container">
                    <% 4.times do |index_y| %>
                      <% 3.times do |index_x| %>

                      <div class="BlockWrapper BlockWrapper--isDropzone draggable-dropzone--occupied vegetable-units">
                        <span><div class="BlockContent"></div></span>

<!-- LEGUME -->
                        <a href="#" class="Block--isDraggable vegetable" title="Click to plant" style="">
                          <div class="BlockContent">
                            <h3>O</h3>
                          </div>
                        </a>
<!-- LEGUME -->

                      </div>
                      <% end %>
                    <% end %>
                  </div>
  </article>
</section>
